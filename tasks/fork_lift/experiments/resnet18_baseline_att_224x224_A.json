{
    "train_dataset": {
        "images_dir": "Gabelstapler",
        "annotations_file": "annotations/Gabelstapler.json",
        "category_name": "forklift",
        "image_shape": [224, 224],
        "target_shape": [56, 56],
        "stdev": 0.025,
        "is_bmp": false,
        "random_angle": [-0.2, 0.2],
        "random_scale": [0.5, 2.0],
        "random_translate": [-0.2, 0.2]
    },
    "stdev_schedule": {
        "0": 0.025
    },
    "color_jitter": {
        "brightness": 0.05,
        "contrast": 0.05,
        "saturation": 0.05,
        "hue": 0.01
    },
    "test_dataset": {
        "images_dir": "Gabelstapler",
        "annotations_file": "annotations/Gabelstapler.json",
        "category_name": "forklift",
        "image_shape": [224, 224],
        "target_shape": [56, 56],
        "stdev": 0.025,
        "is_bmp": false,
        "random_angle": [-0.3, 0.3],
        "random_scale": [0.5, 2.0],
        "random_translate": [-0.3, 0.3]
    },
    "train_loader": {
        "batch_size": 64,
        "shuffle": true,
        "num_workers": 8,
        "pin_memory": true
    },
    "test_loader": {
        "batch_size": 64,
        "shuffle": true,
        "num_workers": 8,
        "pin_memory": true
    },
    "model": {
        "name": "resnet18_baseline_att",
        "kwargs": {
            "cmap_channels": 18,
            "paf_channels": 42,
            "upsample_channels": 256,
            "num_upsample": 3
        }
    },
    "optimizer": {
        "name": "Adam",
        "kwargs": {
            "lr": 1e-3
        }
    },
    "epochs": 250,
    "lr_schedule": {
        "0": 0.001,
        "75": 0.0001,
        "150": 0.00001
    },
    "checkpoints": {
        "interval": 3
    }
}